<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="veterinaria" 
	namespace="veterinaria.Entities">
	<class name = "Cliente" table = "cliente">
		<id name = "Codigo" column = "cod_cli" type = "int" unsaved-value = "0"> 
			<generator class="native">
        	<param name="sequence">cliente_sequence</param>
      	</generator>
		</id>
		<property name="Nombre" column="nombre" not-null="false" type="string"/>
	</class> 
</hibernate-mapping>

